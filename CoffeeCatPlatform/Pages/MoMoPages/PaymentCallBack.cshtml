@page "/Paymentcallback"
@model CoffeeCatPlatform.Pages.MoMoPages.PaymentCallBackModel
@{
    ViewData["Title"] = "Momo payment";
}

<div class="text-center">
    <div class="container">
        <h1 class="text-success">Thanh Toán Thành Công</h1>
        <p>Order ID: <strong>@Model.response.OrderId</strong></p>
        <p>Amount: <strong>@Model.response.Amount</strong></p>
        <p>Order Info: <strong>@Model.response.OrderInfo</strong></p>
    </div>
</div>